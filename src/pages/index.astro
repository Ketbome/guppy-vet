			/* Smooth scrolling for anchor links */
			html {
				scroll-padding-top: var(--header-height);
			}
		</style>

		<script>
			// Mobile menu toggle
			document.addEventListener('DOMContentLoaded', function() {
				const navToggle = document.querySelector('.nav__toggle');
				const navMenu = document.querySelector('.nav__menu');

				if (navToggle && navMenu) {
					navToggle.addEventListener('click', function() {
						navMenu.classList.toggle('nav__menu--active');
					});
				}

				// Smooth scrolling for navigation links
				const navLinks = document.querySelectorAll('.nav__link');
				navLinks.forEach(link => {
					link.addEventListener('click', function(e) {
						const target = e.currentTarget as HTMLAnchorElement;
						if (target.getAttribute('href')?.startsWith('#')) {
							e.preventDefault();
							const targetId = target.getAttribute('href');
							const targetSection = document.querySelector(targetId!);
							if (targetSection) {
								targetSection.scrollIntoView({
									behavior: 'smooth'
								});
							}
						}
					});
				});

				// Add scroll effect to header
				window.addEventListener('scroll', function() {
					const header = document.querySelector('.header');
					if (header instanceof HTMLElement) {
						if (window.scrollY > 100) {
							header.style.background = 'rgba(255, 255, 255, 0.98)';
						} else {
							header.style.background = 'rgba(255, 255, 255, 0.95)';
						}
					}
				});

				// Happy Clients Carousel
				const carouselTrack = document.querySelector('.carousel-track');
				const carouselCards = document.querySelectorAll('.client-card');
				const prevBtn = document.querySelector('.carousel-btn--prev');
				const nextBtn = document.querySelector('.carousel-btn--next');

				if (carouselTrack && carouselCards.length > 0) {
					let currentIndex = 0;
					const cardWidth = 280 + 32; // card width + gap
					const visibleCards = Math.floor(window.innerWidth / cardWidth);
					const maxIndex = Math.max(0, carouselCards.length - visibleCards);
					
					// Duplicate cards for infinite scroll
					const firstCards = Array.from(carouselCards).slice(0, visibleCards);
					const lastCards = Array.from(carouselCards).slice(-visibleCards);
					
					firstCards.forEach(card => {
						const clone = card.cloneNode(true);
						carouselTrack.appendChild(clone);
					});
					
					lastCards.forEach(card => {
						const clone = card.cloneNode(true);
						carouselTrack.insertBefore(clone, carouselTrack.firstChild);
					});

					// Update transform
					function updateCarousel() {
						const translateX = -(currentIndex + visibleCards) * cardWidth;
						(carouselTrack as HTMLElement).style.transform = `translateX(${translateX}px)`;
					}

					// Next button
					if (nextBtn) {
						nextBtn.addEventListener('click', function() {
							currentIndex++;
							if (currentIndex > maxIndex) {
								currentIndex = 0;
							}
							updateCarousel();
						});
					}

					// Previous button
					if (prevBtn) {
						prevBtn.addEventListener('click', function() {
							currentIndex--;
							if (currentIndex < 0) {
								currentIndex = maxIndex;
							}
							updateCarousel();
						});
					}

					// Auto-scroll
					let autoScrollInterval: string | number | NodeJS.Timeout | undefined;
					
					function startAutoScroll() {
						autoScrollInterval = setInterval(function() {
							currentIndex++;
							if (currentIndex > maxIndex) {
								currentIndex = 0;
							}
							updateCarousel();
						}, 4000);
					}

					function stopAutoScroll() {
						clearInterval(autoScrollInterval);
					}

					// Start auto-scroll
					startAutoScroll();

					// Pause auto-scroll on hover
					carouselTrack.addEventListener('mouseenter', stopAutoScroll);
					carouselTrack.addEventListener('mouseleave', startAutoScroll);

					// Pause auto-scroll when buttons are hovered
					if (prevBtn) {
						prevBtn.addEventListener('mouseenter', stopAutoScroll);
						prevBtn.addEventListener('mouseleave', startAutoScroll);
					}
					
					if (nextBtn) {
						nextBtn.addEventListener('mouseenter', stopAutoScroll);
						nextBtn.addEventListener('mouseleave', startAutoScroll);
					}

					// Initialize carousel position
					updateCarousel();

					// Handle window resize
					window.addEventListener('resize', function() {
						const newVisibleCards = Math.floor(window.innerWidth / cardWidth);
						if (newVisibleCards !== visibleCards) {
							location.reload(); // Simple solution for resize
						}
					});

					// Touch/swipe support for mobile
					let startX = 0;
					let startY = 0;
					let isScrolling = false;

					carouselTrack.addEventListener('touchstart', function(e) {
						startX = (e as TouchEvent).touches[0].clientX;
						startY = (e as TouchEvent).touches[0].clientY;
						isScrolling = false;
						stopAutoScroll();
					});

					carouselTrack.addEventListener('touchmove', function(e) {
						if (!startX || !startY) return;

						const diffX = startX - (e as TouchEvent).touches[0].clientX;
						const diffY = startY - (e as TouchEvent).touches[0].clientY;

						if (Math.abs(diffX) > Math.abs(diffY)) {
							isScrolling = true;
							e.preventDefault();
						}
					});

					carouselTrack.addEventListener('touchend', function(e) {
						if (!isScrolling) {
							startAutoScroll();
							return;
						}

						const diffX = startX - (e as TouchEvent).changedTouches[0].clientX;

						if (Math.abs(diffX) > 50) {
							if (diffX > 0) {
								// Swipe left - next
								currentIndex++;
								if (currentIndex > maxIndex) {
									currentIndex = 0;
								}
							} else {
								// Swipe right - previous
								currentIndex--;
								if (currentIndex < 0) {
									currentIndex = maxIndex;
								}
							}
							updateCarousel();
						}

						startAutoScroll();
						startX = 0;
						startY = 0;
						isScrolling = false;
					});
				}
			});
		</script>
